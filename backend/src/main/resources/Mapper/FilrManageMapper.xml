<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.milkyship.backend.FileManageMapper">

	<resultMap id="modelTypeMap" type="com.milkyship.backend.model.fileSystem.FSObject">
		<discriminator javaType="java.lang.Boolean" column="isDir">
			<case value="true" resultType="com.milkyship.backend.model.fileSystem.NetDirectory"/>
			<case value="false" resultType="com.milkyship.backend.model.fileSystem.NetFile"/>
		</discriminator>
	</resultMap>

	<select id="isDir" resultType="boolean">
		SELECT isDir FROM fileList WHERE id=#{FSObjId}
	</select>

<!--	<insert id="add"-->
<!-- TODO: mapper-->

</mapper>